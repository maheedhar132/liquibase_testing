def yamlInput
pipeline{
agent any
stages{
    stage('read yaml'){
        steps{
            script {
            yamlInput = readYaml file: "${WORKSPACE}/input.yaml"
            echo "Yamlfile: " + yamlInput
            def var=yamlInput.getClass()
            print(yamlInput.app.id)
            }
        print(yamlInput)
    }
    }
    stage('test yaml'){
        steps{
            echo "YAML"
            print(yamlInput)
        }
    }
}
}
