def yamlInput
pipeline{
agent any
stages{
    stage('read yaml'){
        steps{
            script{
            yamlInput = readYaml file: "${WORKSPACE}/input.yaml"
            echo "Yamlfile: " + yamlInput
            }
    }
    }
    stage('test yaml'){
        steps{
            echo "YAML"
        }
    }
}
}
